{
  "expo": {
    "name": "Taco Loco",
    "slug": "taco-loco",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "scheme": "tacoloco",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.signemammoth.tacoloco",
      "supportsTablet": true,
      "buildNumber": "1"
    },
    "android": {
      "package": "com.signemammoth.tacoloco",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b4e75eff-7398-4ef7-a12e-f680ced70e9c"
      }
    },
    "owner": "signemammoth"
  }
}
